{
  "name": "vue2-mulit-mobile",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build-report": "npm run build --report",
    "dev": "npm run serve",
    "serve": "vue-cli-service --host 0.0.0.0 serve",
    "start:demo1": "cross-env PRODUCT_NAME=demo1 SERVER_ENV=dev npm run dev",
    "build:demo1": "cross-env PRODUCT_NAME=demo1 SERVER_ENV=prd npm run build",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "commit": "git-cz"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "@babel/runtime-corejs2": "^7.6.3",
    "axios": "^0.18.0",
    "core-js": "^2.6.10",
    "fastclick": "^1.0.6",
    "vant": "^2.10.9",
    "vue": "^2.5.17",
    "vue-infinite-loading": "^2.4.5",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1",
    "vuex-persistedstate": "^3.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.4",
    "@babel/runtime": "^7.5.5",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@vue/cli-plugin-eslint": "^3.8.0",
    "@vue/cli-service": "^3.8.4",
    "@vue/cli-shared-utils": "^5.0.8",
    "@vue/eslint-config-standard": "^3.0.5",
    "babel-eslint": "^7.2.3",
    "babel-plugin-import": "^1.8.0",
    "cross-env": "^5.2.1",
    "glob": "^7.1.7",
    "husky": "^4.2.5",
    "less": "^4.1.3",
    "less-loader": "^5.0.0",
    "vue-template-compiler": "^2.7.14"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "{src,test}/**/*.ts": [
      "prettier --write",
      "git add"
    ]
  }
}
